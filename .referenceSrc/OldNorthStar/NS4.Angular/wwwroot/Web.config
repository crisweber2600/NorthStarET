    <compilation debug="true" targetFramework="4.8"/>
    <httpRuntime targetFramework="4.6"/>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  <system.webServer>
    <urlCompression doStaticCompression="false" doDynamicCompression="false"/>
